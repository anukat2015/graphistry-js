{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    },
    "source": {
        "include": ["src/index.js"]
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/jsdoc-babel"
    ],
    "opts": {
        "encoding": "utf8",
        "private": false,
        "recurse": true
    },
    "babel": {
        "presets": ["es2015", "stage-0"]
    }
}
